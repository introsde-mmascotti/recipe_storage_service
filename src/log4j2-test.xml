<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="ERROR">
	<Appenders>
		<Console name="Console_debug" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} %level %l - %msg%n" />
		</Console>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} %level - %msg%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console_debug" />
		</Root>
	</Loggers>
</Configuration>